type
Student
{
   str name;
   int marks;
}
endtype

int main()
{
   decl
   	int val;
   	Student student;
   enddecl
   begin
   	val = initialize();
   	student = alloc();
   	student.name = "Fahadh";
   	student.marks = 90;
   	write(student.name);
   	write(student.marks);
   	return 0;
   end
}
